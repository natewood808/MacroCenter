@* @model IEnumerable<Product> View expects a sequence of Product objects *@
@model ProductsListViewModel
@foreach (var p in Model.Products)
{
    @await Html.PartialAsync("ProductSummary", p) // Use PartialAsync rather than Partial method since the synchronous, Partial method, may cause deadlocks in the app.
}

<div page-model="@Model.PagingInfo" 
     page-action="List" 
     page-classes-enabled="true"
     page-class="btn" 
     page-class-normal="btn-default"
     page-class-selected="btn-primary" 
     page-url-category="@Model.CurrentCategory"
     page-url-price="@Model.CurrentPriceSort"
     class="btn-group float-right">
</div>